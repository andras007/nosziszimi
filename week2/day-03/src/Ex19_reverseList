public class Ex19_reverseList{
  public static void main(String[] args){
    int[] aj = {3,4,5,6,7};
    int[] temp = new int[aj.length];
    
    for (int i = 0; i < aj.length; i++){
      temp[i] = aj[i];
    }
    
    for (int i = 0; i < aj.length; i++){
      aj[i] = temp[temp.length-i-1];
    }
    
    for (int i = 0; i < aj.length; i++){
      System.out.println(aj[i]);
    }
  }
}
